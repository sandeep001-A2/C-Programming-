#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){
    char ch;
scanf("%c", &ch);
if(ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U' || ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u'){
printf("Vowel");
}
else if ((ch>=65 && ch<=90) ||(ch>=97 && ch<=122)){
    printf("Consonant");
}
    else{
        printf("Invalid Input");
    }
    return 0;
}
